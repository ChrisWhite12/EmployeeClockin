<h1>Show</h1>

<form action = <%=employees_path%> method = "GET">
    <div>
        <label>First Name: </label>
        <p><%= @employee[:fname]%></p>
    </div>
    <div>
        <label>Last Name: </label>
        <p><%= @employee[:lname]%></p>
    </div>
    <div>
        <label>Email: </label>
        <p><%= @employee[:email_address]%></p>
    </div>
    <div>
        <label>Phone: </label>
        <p><%= @employee[:phone]%></p>
    </div>

    <input type="submit">
</form>